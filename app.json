{
  "name": "SASAKI-MD",
  "description": "SASAKI-MD buttoned version of SASAKI.",
  "logo": "https://raw.githubusercontent.com/Alp24ni/Alp24ni/main/Images/SASAKI-MD-logo.png",
  "keywords": ["TalkDrove", "bot", "whatsapp bot", "multi device", "SASAKI-MD"],
  "success_url": "/",

  "env": {
    "SESSION_ID": {
            "description": "Paste your session ID here (Make sure starts with Sasaki;;;) or leave empty if deploying through Creds.json",
            "required": true,
            "value": ""
        },
    "COMMAND_TYPE": {
            "description": "Put here (button or nonbutton) to change bot command mode",
            "required": true,
            "value": "button"
        },
        "ANTI_DELETE": {
            "description": "Put true to enable otherwise put false",
            "required": false,
            "value": "false"
        },
        "AUTO_STATUS_READ": {
            "description": "Put true to enable otherwise put false",
            "required": false,
            "value": "false"
        },
        "ALWAYS_ONLINE": {
            "description": "Put true to enable otherwise put false",
            "required": false,
            "value": "false"
        },
        "AUTO_REACT": {
            "description": "Put true to enable otherwise put false",
            "required": false,
            "value": "false"
        },
        "HEROKU_APP_NAME": {
            "description": "Put your HEROKU app name.",
            "required": false,
            "value": ""
        },
        "HEROKU_API_KEY": {
            "description": "Put HEROKU api key.",
            "required": false,
            "value": ""
        },
        "WORK_TYPE": {
            "description": "Put public or private",
            "required": false,
            "value": "public"
        },
    "OWNER_NUMBER": {
            "description": "Put here your number without +",
            "required": true,
            "value": "22176xxxxxxx"
        }
    },
    "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        }
     ],
  "stack": "heroku-22"
}
